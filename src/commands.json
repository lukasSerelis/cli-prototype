{
  "welcome": "Welcome to My CLI! Type 'help' for available commands.",
  "commands": {
    "juju ui": {
      "description": "Generates link to view dashboard of this resource",
      "handler": [
        {
          "component": "spinner",
          "output": " Generating UI link...",
          "duration": 1000,
          "conclusion": "succeed"
        },
        {
          "component": "table",
          "output": [
            [
              "Juju dashboard UI link"
            ],
            [
              "\u001b[4mlocalhost/controller1\u001b[0m"
            ]
          ],
          "colWidths": [
            100
          ]
        }
      ]
    }
  }
}
