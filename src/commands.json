{
    "welcome": "Welcome to Juju CLI. Type 'help' for command list",
    "variables": {
        "isLoggedIn": "true"
    },
    "commands": {
        "ui": {
            "description": "Launch the Juju web UI dashboard",
            "handler": [
                {
                    "component": "spinner",
                    "output": "Loading Juju Dashboard UI...",
                    "duration": 1500
                },
                {
                    "component": "text",
                    "output": "Dashboard UI Available At:\n\nhttps://juju.example.com/dashboard"
                },
                {
                    "component": "text",
                    "output": "\nYou can access the full Juju Dashboard at the URL above."
                }
            ]
        }
    }
}
