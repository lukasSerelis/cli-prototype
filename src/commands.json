{
  "welcome": "Welcome to Juju CLI! Type 'help' for available commands.",
  "commands": {
    "juju --ui": {
      "description": "Generates a juju dashboard link for the current resource",
      "handler": [
        {
          "component": "spinner",
          "output": "Creating juju UI link",
          "duration": 1000,
          "conclusion": "succeed"
        },
        {
          "component": "table",
          "output": [
            [
              "Juju Dashboard UI Link"
            ],
            [
              "domain.juju/controller1"
            ]
          ]
        }
      ]
    }
  }
}
