{
  "json-key">"welcome": "Welcome to my CLI prototype!",
  "json-key">"variables": {},
  "json-key">"commands": {
    "json-key">"juju ui": {
      "json-key">"description": "Open Juju dashboard UI",
      "json-key">"handler": [
        {
          "json-key">"component": "select",
          "json-key">"name": "controller",
          "json-key">"message": "Select a controller:",
          "json-key">"choices": [
            "controller-aws",
            "controller-gcp",
            "controller-azure"
          ]
        },
        {
          "json-key">"component": "select",
          "json-key">"name": "model",
          "json-key">"message": "Select a model:",
          "json-key">"choices": [
            "model-production",
            "model-staging",
            "model-development"
          ]
        },
        {
          "json-key">"component": "table",
          "json-key">"output": [
            [
              "juju dashboard ui"
            ],
            [
              "https://{{controller}}.juju-dashboard.com/model/{{model}}"
            ]
          ]
        }
      ]
    }
  }
}
